            <Field code="ZJ_CONT_TYPE" name="合同类型资金"  type="enum" enum="'融资顾问合同','授信合同','借款合同','其他合同'"/>
            <Field code="PREFIX" name="编码前缀"  type="string"/>
            <Field code="OLD_CODE" name="原编码"  type="string"/>
            <Field code="IF_DRAWINGS" name="是否分次提款"  type="enum" enum="'是','否'"/>
            <Field code="BILLVO" name="单据VO"  type="string" />
