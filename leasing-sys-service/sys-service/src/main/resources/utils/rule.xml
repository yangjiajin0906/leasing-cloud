            <Field code="MONTH" name="月份"  type="string"/>
            <Field code="DATE" name="日期"  type="string"/>
            <Field code="LIQUID" name="顺序号"  type="string"/>
            <Field code="FIXDATE" name="指定日期"  type="string"/>
            <Field code="CAPITALBUSICODE" name="资金业务代码"  type="string"/>
            <Field code="CONTRACTCODE" name="合同编号"  type="string"/>
            <Field code="BORROWCUSTOMERSHORT" name="借入方简称"  type="string"/>
            <Field code="FINANCECUSTOMERCODE" name="金融机构编号"  type="string"/>
            <Field code="USERCODE" name="柜员号"  type="string"/>
            <Field code="OPERATOR" name="操作员"  type="string"/>
            <Field code="CUSTOMERCODE" name="客户编号"  type="string"/>
            <Field code="CUSTOMERNAME" name="客户名称"  type="string"/>
